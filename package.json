    "@types/react": "^16.0.33",
    "babel-jest": "22.0.4",
    "babel-preset-react-native": "4.0.0",
    "concurrently": "^3.5.1",
    "jest": "22.0.4",
    "react-test-renderer": "16.0.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "preset": "react-native"
  }
